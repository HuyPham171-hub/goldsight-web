{
  "buildCommand": "bash build.sh",
  "outputDirectory": "dist",
  "installCommand": "pip install -r requirements.txt",
  "framework": null,
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
